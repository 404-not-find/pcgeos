chunk TextChunk = data {
text_base	label	char
L1	label	char
	char	"\r"
L2	label	char
	char	"B"
L3	label	char
	char	"OBBIN"
L4	label	char
	char	"\r"
	char	"The creator of this screen saver was inspired by a sweater a"
	char	"nd a nail. Just so you know"
	char	C_EMDASH
	char	"the guy with the unraveling machine, his name"
	char	C_QUOTESNGRIGHT
	char	"s Oomao."
L5	label	char
	char	"\r"
L6	label	char
	char	C_INFINITY
	char	C_NULL
}
chunk TextStyleRuns = data {
    RunArray <
	TextStyleElements,	; RA_elementArrayChunk
	0,			; RA_elementArrayHandle (set later)
	0,			; RA_nameArrayChunk
	0,			; RA_nameArrayHandle (set later)
	0			; RA_last
    >
    RunArrayElement \
	<L1-text_base, TSE_Style4-TSE_base>,
	<L2-text_base, TSE_Style2-TSE_base>,
	<L3-text_base, TSE_Style4-TSE_base>,
	<L4-text_base, TSE_Style3-TSE_base>,
	<L5-text_base, TSE_Style6-TSE_base>,
	<L6-text_base, TSE_Style5-TSE_base>,
	<TEXT_ADDRESS_PAST_END, 0>
}
chunk TextRulerRuns = data {
    RunArray <
	TextRulerElements,	; RA_elementArrayChunk
	0,			; RA_elementArrayHandle (set later)
	0,			; RA_nameArrayChunk
	0,			; RA_nameArrayHandle (set later)
	0			; RA_last
    >
    RunArrayElement \
	<L1-text_base, TEXT_RULER_1>,
	<L2-text_base, TEXT_RULER_2>,
	<L6-text_base, TEXT_RULER_3>,
	<TEXT_ADDRESS_PAST_END, 0>
}
