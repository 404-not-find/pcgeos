        0, ; VTR_reserved[0]
        0, ; VTR_reserved[1]
        0, ; VTR_reserved[2]
        0 ; VTR_reserved[3]
    >
>
}
chunk TextChunk = data {
text_base	label	char
L1	label	char
	char	"\r"
L2	label	char
	char	"Template\r"
L3	label	char
	char	"Help for Template screen saver module.\r"
L4	label	char
	char	C_INFINITY
L5	label	char
	char	"\r"
	char	"\r"
	char	"\r"
	char	C_NULL
}
chunk TextStyleRuns = data {
    RunArray <
	TextStyleElements,	; RA_elementArrayChunk
	0,			; RA_elementArrayHandle (set later)
	0,			; RA_nameArrayChunk
	0,			; RA_nameArrayHandle (set later)
	0			; RA_last
    >
    RunArrayElement \
	<L1-text_base, TSE_Style2-TSE_base>,
	<L2-text_base, TSE_Style3-TSE_base>,
	<L3-text_base, TSE_Style2-TSE_base>,
	<L4-text_base, TSE_Style4-TSE_base>,
	<L5-text_base, TSE_Style2-TSE_base>,
	<TEXT_ADDRESS_PAST_END, 0>
}
chunk TextRulerRuns = data {
    RunArray <
	TextRulerElements,	; RA_elementArrayChunk
	0,			; RA_elementArrayHandle (set later)
	0,			; RA_nameArrayChunk
	0,			; RA_nameArrayHandle (set later)
	0			; RA_last
    >
    RunArrayElement \
	<L1-text_base, TEXT_RULER_2>,
	<TEXT_ADDRESS_PAST_END, 0>
}
