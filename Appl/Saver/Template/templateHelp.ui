chunk TextRulerElements = data {
TRE_base ElementArray <2,3,0>
TEXT_RULER_1 equ 1
VisTextRuler< ; VisTextRuler
    < ; VTR_meta
        3, ; E_refCount
        0 ; E_id
    >,
    1, ; VTR_token
    < ; VTR_border
        0, ; VTPBF_LEFT
        0, ; VTPBF_TOP
        0, ; VTPBF_RIGHT
        0, ; VTPBF_BOTTOM
        0, ; VTPBF_DOUBLE
        0, ; VTPBF_DRAW_INNER_LINES
        0, ; VTPBF_WIDTH
        0, ; VTPBF_SPACING
        0, ; VTPBF_SHADOW
        SA_TOP_LEFT ; VTPBF_ANCHOR
    >,
    <SCP_index ; VTR_borderColor.SCP_index
        < ; SCP_index
            BLACK, ; SCPI_index
            < ; SCPI_info
                0, ; VTCMM_COLOR_FLAG
                VTGS_100, ; VTCMM_GRAY_SCREEN
                0, ; VTCMM_TRANSPARENT
                0, ; VTCMM_ON_BLACK
                COLOR_MAP_DITHER ; VTCMM_MAP_MODE
            >,
            0 ; SCPI_unused
        >
    >,
    < ; VTR_attributes
        J_LEFT, ; VTRA_JUSTIFICATION
        VTDT_INCH ; VTRA_DEFAULT_TABS
    >,
    0, ; VTR_leftMargin
    0, ; VTR_rightMargin
    0, ; VTR_paraMargin
    < ; VTR_lineSpacing
        0, ; BBF_frac
        1 ; BBF_int
    >,
    0, ; VTR_leading
    < ; VTR_spaceOnTop
        0, ; BBF_frac
        0 ; BBF_int
    >,
    < ; VTR_spaceOnBottom
        0, ; BBF_frac
        0 ; BBF_int
    >,
    <SCP_index ; VTR_bgColor.SCP_index
        < ; SCP_index
            WHITE, ; SCPI_index
            < ; SCPI_info
                0, ; VTCMM_COLOR_FLAG
                VTGS_100, ; VTCMM_GRAY_SCREEN
                0, ; VTCMM_TRANSPARENT
                0, ; VTCMM_ON_BLACK
                COLOR_MAP_DITHER ; VTCMM_MAP_MODE
            >,
            0 ; SCPI_unused
        >
    >,
    0, ; VTR_numberOfTabs
    < ; VTR_reserved
        0, ; VTR_reserved[0]
        0, ; VTR_reserved[1]
        0, ; VTR_reserved[2]
        0 ; VTR_reserved[3]
    >
>
TEXT_RULER_2 equ 3
VisTextRuler< ; VisTextRuler
    < ; VTR_meta
        1, ; E_refCount
        0 ; E_id
    >,
    3, ; VTR_token
    < ; VTR_border
        0, ; VTPBF_LEFT
        0, ; VTPBF_TOP
        0, ; VTPBF_RIGHT
        0, ; VTPBF_BOTTOM
        0, ; VTPBF_DOUBLE
        0, ; VTPBF_DRAW_INNER_LINES
        0, ; VTPBF_WIDTH
        0, ; VTPBF_SPACING
        0, ; VTPBF_SHADOW
        SA_TOP_LEFT ; VTPBF_ANCHOR
    >,
    <SCP_index ; VTR_borderColor.SCP_index
        < ; SCP_index
            BLACK, ; SCPI_index
            < ; SCPI_info
                0, ; VTCMM_COLOR_FLAG
                VTGS_100, ; VTCMM_GRAY_SCREEN
                0, ; VTCMM_TRANSPARENT
                0, ; VTCMM_ON_BLACK
                COLOR_MAP_DITHER ; VTCMM_MAP_MODE
            >,
            0 ; SCPI_unused
        >
    >,
    < ; VTR_attributes
        J_LEFT, ; VTRA_JUSTIFICATION
        VTDT_INCH ; VTRA_DEFAULT_TABS
    >,
    0, ; VTR_leftMargin
    0, ; VTR_rightMargin
    0, ; VTR_paraMargin
    < ; VTR_lineSpacing
        0, ; BBF_frac
        1 ; BBF_int
    >,
    0, ; VTR_leading
    < ; VTR_spaceOnTop
        0, ; BBF_frac
        0 ; BBF_int
    >,
    < ; VTR_spaceOnBottom
        0, ; BBF_frac
        7 ; BBF_int
    >,
    <SCP_index ; VTR_bgColor.SCP_index
        < ; SCP_index
            WHITE, ; SCPI_index
            < ; SCPI_info
                0, ; VTCMM_COLOR_FLAG
                VTGS_100, ; VTCMM_GRAY_SCREEN
                0, ; VTCMM_TRANSPARENT
                0, ; VTCMM_ON_BLACK
                COLOR_MAP_DITHER ; VTCMM_MAP_MODE
            >,
            0 ; SCPI_unused
        >
    >,
