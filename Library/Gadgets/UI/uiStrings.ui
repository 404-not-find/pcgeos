chunk   JS_ShortMonthShortYear  = "|SM| |SY|";
chunk	BatteryEmptyFullLetterString = "EF";
localize "This 2 character string specifies the letters to draw at the ends " \
	 "of the Battery level indicators.  The first character should "\
         "stand for 'E'mpty, the second for 'F'ull";
end	GadgetsStrings;






