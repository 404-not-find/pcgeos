cvtpcx -f -m13 -t -z -x0 -y96  -nBar fgcalc.pcx
cvtpcx -f -m13 -t -z -x0 -y112 -nLine fgcalc.pcx
cvtpcx -f -m13 -t -z -x0 -y128 -nArea fgcalc.pcx
cvtpcx -f -m13 -t -z -x0 -y144 -nScatter fgcalc.pcx
cvtpcx -f -m13 -t -z -x0 -y160 -nPie fgcalc.pcx
cvtpcx -f -m13 -t -z -x0 -y208 -nHighLow fgcalc.pcx
cvtpcx -f -m13 -t -z -x0 -y224 -nSpider fgcalc.pcx
