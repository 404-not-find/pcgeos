@visMoniker DatePlusMoniker = {
	size = standard;
	style = icon;
	color = gray1;
	aspectRatio = normal;
	cachedSize = 6, 6;
	gstring {
		GSFillBitmapAtCP (12),
		Bitmap (6,6,BMC_UNCOMPACTED, BMF_MONO),
		0x30, 0x30, 0x78, 0x78, 0xfc, 0xfc,
		GSEndString()
	}
}

@visMoniker DateMinusMoniker = {
	size = standard;
	style = icon;
	color = gray1;
	aspectRatio = normal;
	cachedSize = 6, 6;
	gstring {
		GSFillBitmapAtCP (12),
		Bitmap (6,6,BMC_UNCOMPACTED, BMF_MONO),
		0xfc, 0xfc, 0x78, 0x78, 0x30, 0x30,
		GSEndString()
	}
}





