@start  AppResource;
@object GenApplicationClass TAApp = {
    GI_visMoniker = list {
			@TATextMoniker,
			@TALCAppMoniker,
         @TATCAppMoniker
     }
    GI_comp = @TAPrimary;
    gcnList(MANUFACTURER_ID_GEOWORKS,GAGCNLT_WINDOWS) =
                @TAPrimary;
    gcnList(MANUFACTURER_ID_GEOWORKS,MGCNLT_ACTIVE_LIST) =
                @TADocumentControl, @TADisplayControl;
    gcnList(MANUFACTURER_ID_GEOWORKS,MGCNLT_APP_STARTUP) =
                @TADocumentControl;
    gcnList( MANUFACTURER_ID_GEOWORKS, GAGCNLT_SELF_LOAD_OPTIONS ) =
			 @TADocumentControl, @TADisplayControl, @TASeatingPrintControl;
    ATTR_GEN_APPLICATION_PRINT_CONTROL = @TASeatingPrintControl;
    ATTR_GEN_HELP_FILE = "TAHelp";
}

@visMoniker TATextMoniker = "Teacher's Aide";
@localize "Application name displayed on title bar";

@end AppResource


@start  Interface;
@object GenPrimaryClass TAPrimary = {
    GI_comp = @FileMenu, @MenuBar, @TADisplayGroup, @TADialogs;
    GI_attrs = @default | GA_TARGETABLE
                    | GA_KBD_SEARCH_PATH;
    ATTR_GEN_HELP_CONTEXT = "TOC";
}

@object GenInteractionClass FileMenu = {
    GI_comp = @TADocumentControl, @TASeatingPrintControl,
		    @ImpExSubGroup, @AboutDBox;
    GI_attrs = @default | GA_TARGETABLE
                    | GA_KBD_SEARCH_PATH;
    GII_visibility = GIV_POPUP;
    ATTR_GEN_INTERACTION_GROUP_TYPE = (GIGT_FILE_MENU);
}

@object GenDocumentControlClass TADocumentControl = {
    GI_attrs = @default | GA_TARGETABLE
                    | GA_KBD_SEARCH_PATH;
    GDCI_attrs = @default | GDCA_MULTIPLE_OPEN_FILES;
    GDCI_features = @default & ~GDCF_NAME_ON_PRIMARY
                        & ~GDCF_SUPPORTS_TEMPLATES;
	 GDCI_documentToken = {"TAd1", 16431};
    GDCI_noNameText = "No File";
    @localize "Shown on title bar if no file is acitve";
    GDCI_documentGroup = TADocumentGroup;
    GDCI_enableDisableList = TAEnableList;
}

@chunk optr TAEnableList[] = {
    @StudentsMenu, @SetUpMenu, @ModulesMenu,
    @GPMenu, @ToolMenu, @ImpExSubGroup
};

@object PrintControlClass TASeatingPrintControl = {
    GI_states = @default | GS_ENABLED;
    PCI_attrs = (@default) & ~PCA_PAGE_CONTROLS;
    ATTR_GEN_INTERACTION_GROUP_TYPE = GIGT_PRINT_GROUP;
    PCI_output = (TO_APP_MODEL);
    PCI_docNameOutput = @TADocumentGroup;
    PCI_docSizeInfo   = {(8*72),   /* width */
			 (10.5*72),    /* height */
			 PT_PAPER,    /* layout */
			 {(.25)*72, (.25)*72, (.25)*72, (.25)*72}};  /* margins */
    ATTR_GEN_CONTROL_PROHIBIT_UI = PRINTCF_FAX_TRIGGER;
}

@object GenInteractionClass ImpExSubGroup = {
    GI_comp = @ImportTrigger, @ExportTrigger;
	 GI_states = @default & ~GS_ENABLED;
	 GII_visibility = GIV_SUB_GROUP;
}

@object GenTriggerClass ImportTrigger = {
    GI_visMoniker = 'I', "Import";
    @localize "Opens Import dialog box";
    GTI_destination = (TO_APP_MODEL);
    GTI_actionMsg = MSG_OPEN_IMPORT;
    ATTR_GEN_DESTINATION_CLASS = {(ClassStruct *)&TADocumentClass};
    HINT_TRIGGER_BRINGS_UP_WINDOW;
}

@object GenTriggerClass ExportTrigger = {
    GI_visMoniker = 'E', "Export";
    @localize "Opens Export dialog box";
