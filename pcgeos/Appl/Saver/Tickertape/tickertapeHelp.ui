chunk TextChunk = data {
text_base	label	char
L1	label	char
	char	"T"
L2	label	char
	char	"ICKER TAPE"
L3	label	char
	char	"\r"
L4	label	char
	char	"This is your chance to make your own ticker tape parade"
	char	C_EMDASH
	char	"and you don"
	char	C_QUOTESNGRIGHT
	char	"t even have to be rich and famous, or live in New York. You "
	char	"choose how much tape is thrown, and how big the strands are."
	char	"\r"
L5	label	char
	char	C_INFINITY
	char	C_NULL
}
chunk TextStyleRuns = data {
    RunArray <
	TextStyleElements,	; RA_elementArrayChunk
	0,			; RA_elementArrayHandle (set later)
	0,			; RA_nameArrayChunk
	0,			; RA_nameArrayHandle (set later)
	0			; RA_last
    >
    RunArrayElement \
	<L1-text_base, TSE_Style2-TSE_base>,
	<L2-text_base, TSE_Style3-TSE_base>,
	<L3-text_base, TSE_Style4-TSE_base>,
	<L5-text_base, TSE_Style5-TSE_base>,
	<TEXT_ADDRESS_PAST_END, 0>
}
chunk TextRulerRuns = data {
