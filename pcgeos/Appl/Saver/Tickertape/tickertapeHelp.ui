	0			; RA_last
    >
    RunArrayElement \
	<L1-text_base, TEXT_RULER_2>,
	<L4-text_base, TEXT_RULER_3>,
	<L5-text_base, TEXT_RULER_4>,
	<TEXT_ADDRESS_PAST_END, 0>
}
