start	OptionsASF1BinResource, notDetachable;

ASF1BinOptionsDialogBox = GenInteraction {
	children = 
		ASF1BinInputList,
		ASF1BinMainCountryList;
	genStates = default -usable;
	hints = {
		ATTR_GEN_INIT_FILE_PROPAGATE_TO_CHILDREN
		}
}

ASF1BinInputList = GenItemGroup {
	moniker = "Paper Source:";
	selection =
		"TF_TRACTOR1 shl offset PIO_TRACTOR";
	numSelections = 1;
	children =
		ASF1BinManualEntry,
		ASF1BinTray1Entry,
		ASF1BinTractorEntry;
        hints = {
                HINT_ORIENT_CHILDREN_HORIZONTALLY,
                HINT_ALLOW_CHILDREN_TO_WRAP,
		ATTR_GEN_USES_HIERARCHICAL_INIT_FILE_CATEGORY,
		ATTR_GEN_INIT_FILE_KEY {
			byte "paperSource",0
		}
	}
}


ASF1BinManualEntry = GenItem {
	moniker = "Manual";
        identifier = "MF_MANUAL1 shl offset PIO_MANUAL";
}

ASF1BinTray1Entry = GenItem {
	moniker = "Bin 1";
        identifier = "ASF_TRAY1 shl offset PIO_ASF";
}

ASF1BinTractorEntry = GenItem {
	moniker = "Tractor";
        identifier = "TF_TRACTOR1 shl offset PIO_TRACTOR";
}

ASF1BinMainCountryList = GenInteraction {
        moniker = "International Symbols:";
        children =
                ASF1BinCountryList;
        hints = {
                HINT_ORIENT_CHILDREN_HORIZONTALLY,
                ATTR_GEN_INIT_FILE_PROPAGATE_TO_CHILDREN
                }
}

ASF1BinCountryList = GenItemGroup {
        selection =
                "PCC_USA";
        numSelections = 1;
	children =
		ASF1BinUSAEntry,
		ASF1BinFranceEntry,
		ASF1BinGermanyEntry,
		ASF1BinUnitedKingdomEntry,
		ASF1BinDenmark1Entry,
		ASF1BinSwedenEntry,
		ASF1BinItalyEntry,
		ASF1BinSpain1Entry,
		ASF1BinJapanEntry,
		ASF1BinNorwayEntry,
		ASF1BinDenmark2Entry,
		ASF1BinSpain2Entry,
		ASF1BinLatAmerEntry;
        hints = {
                HINT_ITEM_GROUP_MINIMIZE_SIZE,
                HINT_ITEM_GROUP_DISPLAY_CURRENT_SELECTION,
		ATTR_GEN_USES_HIERARCHICAL_INIT_FILE_CATEGORY,
		ATTR_GEN_INIT_FILE_KEY {
			byte "Country",0
		}
	}
}

ASF1BinUSAEntry = GenItem {
	moniker = "USA";
        identifier = "PCC_USA";
}

ASF1BinFranceEntry = GenItem {
	moniker = "France";
        identifier = "PCC_FRANCE";
}

ASF1BinGermanyEntry = GenItem {
	moniker = "Germany";
        identifier = "PCC_GERMANY";
}

ASF1BinUnitedKingdomEntry = GenItem {
	moniker = "United Kingdom";
        identifier = "PCC_UK";
}

ASF1BinDenmark1Entry = GenItem {
	moniker = "Denmark 1";
        identifier = "PCC_DENMARK1";
}

ASF1BinSwedenEntry = GenItem {
	moniker = "Sweden";
        identifier = "PCC_SWEDEN";
}

ASF1BinItalyEntry = GenItem {
	moniker = "Italy";
        identifier = "PCC_ITALY";
}

ASF1BinSpain1Entry = GenItem {
	moniker = "Spain 1";
        identifier = "PCC_SPAIN1";
}

ASF1BinJapanEntry = GenItem {
	moniker = "Japan";
        identifier = "PCC_JAPAN";
}

ASF1BinNorwayEntry = GenItem {
	moniker = "Norway";
        identifier = "PCC_NORWAY";
}

ASF1BinDenmark2Entry = GenItem {
	moniker = "Denmark 2";
        identifier = "PCC_DENMARK2";
}

ASF1BinSpain2Entry = GenItem {
	moniker = "Spain 2";
        identifier = "PCC_SPAIN2";
}

ASF1BinLatAmerEntry = GenItem {
	moniker = "Latin America";
        identifier = "PCC_LAT_AMERICA";
}


end	OptionsASF1BinResource;

