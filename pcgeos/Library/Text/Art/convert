cvtpcx -2 -f -m13 -t -z -x0 -y224 -nOneAndAHalfSpace stylspac.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y240 -nDoubleSpace stylspac.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y256 -nTripleSpace stylspac.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y272 -nBoxed stylspac.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y288 -nButton stylspac.pcx

cvtpcx -2 -f -m13 -t -z -x0 -y0 -nSpell newdict.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y16 -nHyphenation newdict.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y0 -nSearchReplace search.pcx

cvtpcx -2 -f -R -m13 -t -z -x0 -y96 -j -dTC -dTCGA -nWildCardGString s-rchar.pcx
