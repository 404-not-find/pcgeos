cvtpcx -2 -f -m13 -t -z -x0 -y96 -nStrikethru stylspac.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y112 -nSmaller stylspac.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y128 -nLarger stylspac.pcx
cvtpcx -2 -f -m13 -t -z -x0 -y144 -nCenter stylspac.pcx
