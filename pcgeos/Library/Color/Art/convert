#!/bin/sh
#
#	Conversion script for all art related to the color library
#
#
#	Revision History:
#
#	Name	Date		Description
#	----	----		-----------
#	jon	30 nov 1992	initial revision
#
#	$Id: convert,v 1.1.33.1 97/03/29 04:58:20 canavese Exp $
#

cvtpcx -f -m2 -t -x333 -y1 -w27 -h18 -nAreaColor grobj.pcx
cvtpcx -f -b -m2 -t -x333 -y20 -w27 -h18 -nAreaMask grobj.pcx
cvtpcx -f -b -m2 -t -x333 -y39 -w27 -h18 -nAreaPattern grobj.pcx

cvtpcx -f -m2 -t -x333 -y58 -w27 -h18 -nLineColor grobj.pcx
cvtpcx -f -b -m2 -t -x333 -y77 -w27 -h18 -nLineMask grobj.pcx

cvtpcx -f -m2 -t -x333 -y96 -w27 -h18 -nTextColor grobj.pcx
cvtpcx -f -b -m2 -t -x333 -y115 -w27 -h18 -nTextMask grobj.pcx
cvtpcx -f -b -m2 -t -x333 -y134 -w27 -h18 -nTextPattern grobj.pcx
