SetDef	__FAX_FILE				; prevent double-inclusion

StartLibrary	abook


global	ABookOpenFile:far
global	ABookCloseFile:far
global	ABookEnumerateEntries:far
global	ABookGetInfo:far
global	ABookGetData:far
global	ABookMakeNewElement:far
global	ABookSetData:far

EndLibrary	abook
